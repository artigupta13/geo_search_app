{
  "testEnvironment": "node",
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": ["json", "lcov", "text", "clover"],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/test/"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/"
  ]
}
